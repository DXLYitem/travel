        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.travel.dao.PreorderDao">
    <insert id="insertPreorder" parameterType="Preorder">
        insert into preorder(name,phone,address,status) value (#{name},#{phone},#{address},#{status})
